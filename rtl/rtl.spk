class Integer : Object {
    
    // unary operators
    extern self + 1 {}
    extern self - 1 {}
    extern +self    {}
    extern -self    {}
    extern ~self    {}
    
    // binary operators
    extern self * x {}
    extern self / x {}
    extern self % x {}
    extern self + x {}
    extern self - x {}
    extern self << x {}
    extern self >> x {}
    extern self & x {}
    extern self ^ x {}
    extern self | x {}
    
    // comparison operators
    extern self < x {}
    extern self > x {}
    extern self <= x {}
    extern self >= x {}
    extern self == x {}
    extern self != x {}
    
    // unboxing
    extern unboxed {}
}


